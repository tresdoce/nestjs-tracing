{
  "name": "@tresdoce/nestjs-tracing",
  "version": "0.0.1",
  "description": "NestJS tracing module",
  "author": {
    "name": "Maximiliano \"Mex\" Delgado",
    "email": "mdelgado@tresdoce.com.ar",
    "url": "https://rudemex.github.io/"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "keywords": [
    "nestjs",
    "tracing",
    "jaeger"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tresdoce/nestjs-tracing.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tresdoce/nestjs-tracing/issues"
  },
  "homepage": "https://github.com/tresdoce/nestjs-tracing#readme",
  "private": false,
  "scripts": {
    "prebuild": "shx rm -rf pkg",
    "build": "pika build",
    "lint": "eslint \"{src,apps,libs,test,__test__}/**/*.ts\" --fix",
    "pre-commit": "lint-staged",
    "lint-staged": "lint-staged",
    "publish": "pika publish",
    "test": "jest --runInBand --detectOpenHandles --coverage",
    "release": "standard-version"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.2.0",
    "@nestjs/core": "^8.2.0",
    "@nestjs/platform-express": "^8.2.0",
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/auto-instrumentations-node": "^0.27.3",
    "@opentelemetry/core": "^1.0.1",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/exporter-collector-grpc": "^0.25.0",
    "@opentelemetry/exporter-jaeger": "^1.0.1",
    "@opentelemetry/exporter-prometheus": "^0.27.0",
    "@opentelemetry/host-metrics": "^0.27.1",
    "@opentelemetry/propagator-b3": "^1.0.1",
    "@opentelemetry/propagator-jaeger": "^1.0.1",
    "@opentelemetry/sdk-metrics-base": "^0.27.0",
    "@opentelemetry/sdk-node": "^0.27.0",
    "@opentelemetry/sdk-trace-base": "^1.0.1",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "@opentelemetry/instrumentation-aws-sdk": "^0.5.0",
    "@opentelemetry/instrumentation-cassandra-driver": "^0.27.1",
    "@opentelemetry/instrumentation-knex": "^0.27.1",
    "@opentelemetry/instrumentation-mysql2": "^0.28.0",
    "@opentelemetry/instrumentation-winston": "^0.27.1",
    "aws-sdk": "^2.1072.0",
    "cassandra-driver": "^4.6.3",
    "knex": "^1.0.2",
    "mysql2": "^2.3.3",
    "winston": "^3.5.1",
    "winston2": "^1.0.3",
    "lodash": "^4.17.21",
    "opentelemetry-node-metrics": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/platform-express": "^8.2.6",
    "@opentelemetry/api": "^1.0.4",
    "@opentelemetry/auto-instrumentations-node": "^0.27.3",
    "@opentelemetry/core": "^1.0.1",
    "@opentelemetry/exporter-collector": "^0.25.0",
    "@opentelemetry/exporter-collector-grpc": "^0.25.0",
    "@opentelemetry/exporter-jaeger": "^1.0.1",
    "@opentelemetry/exporter-prometheus": "^0.27.0",
    "@opentelemetry/host-metrics": "^0.27.1",
    "@opentelemetry/propagator-b3": "^1.0.1",
    "@opentelemetry/propagator-jaeger": "^1.0.1",
    "@opentelemetry/sdk-metrics-base": "^0.27.0",
    "@opentelemetry/sdk-node": "^0.27.0",
    "@opentelemetry/sdk-trace-base": "^1.0.1",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "lodash": "^4.17.21",
    "opentelemetry-node-metrics": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4",
    "shx": "^0.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-proposal-optional-chaining": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.2.0",
    "@pika/cli": "^0.2.0",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@types/express": "^4.17.11",
    "@types/jest": "27.4.0",
    "@types/node": "^17.0.16",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^12.3.3",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "printWidth": 100,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && jest"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "**/__test__/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node",
        {
          "exclude": [
            "**/__test__/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-types",
        {
          "exclude": [
            "**/__test__/*"
          ]
        }
      ]
    ]
  }
}
